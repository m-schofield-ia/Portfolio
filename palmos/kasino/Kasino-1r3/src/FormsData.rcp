FORM ID fNewGame AT (0 0 160 160)
NOFRAME
DEFAULTBTNID fNewGameBegin
MENUID sysEditMenuID
BEGIN
	TITLE "@@fNGTitle@@"
	FIELD ID fNewGameName1 AT (49 19 80 12) EDITABLE UNDERLINED MAXCHARS MaxNameLength
	LABEL "@@fNGPly1@@:" AUTOID AT (RIGHT@45 19)
	BUTTON "\x0B" ID fNewGameSelect1 AT (132 19 10 AUTO) NOFRAME FONT 3
	FORMBITMAP AT (144 19) BITMAP bTrash
	GADGET ID fNewGameClear1 AT (144 19 13 9 )

	FIELD ID fNewGameName2 AT (49 33 80 12) EDITABLE UNDERLINED MAXCHARS MaxNameLength
	LABEL "@@fNGPly2@@:" AUTOID AT (RIGHT@45 33)
	BUTTON "\x0B" ID fNewGameSelect2 AT (132 33 10 AUTO) NOFRAME FONT 3
	FORMBITMAP AT (144 33) BITMAP bTrash
	GADGET ID fNewGameClear2 AT (144 33 13 9 )

	FIELD ID fNewGameName3 AT (49 47 80 12) EDITABLE UNDERLINED MAXCHARS MaxNameLength
	LABEL "@@fNGPly3@@:" AUTOID AT (RIGHT@45 47)
	BUTTON "\x0B" ID fNewGameSelect3 AT (132 47 10 AUTO) NOFRAME FONT 3
	FORMBITMAP AT (144 47) BITMAP bTrash
	GADGET ID fNewGameClear3 AT (144 47 13 9 )

	FIELD ID fNewGameName4 AT (49 61 80 12) EDITABLE UNDERLINED MAXCHARS MaxNameLength
	LABEL "@@fNGPly4@@:" AUTOID AT (RIGHT@45 61)
	BUTTON "\x0B" ID fNewGameSelect4 AT (132 61 10 AUTO) NOFRAME FONT 3
	FORMBITMAP AT (144 61) BITMAP bTrash
	GADGET ID fNewGameClear4 AT (144 61 13 9 )

	LABEL "@@fNGRounds@@:" AUTOID AT (RIGHT@52 85)
	POPUPTRIGGER "" ID fNewGameRoundsPopup AT (56 85 AUTO AUTO) LEFTANCHOR
	LIST "@@fNGUnlimited@@" "5" "10" "15" "20" "25" "50" "@@fNGOther@@ ..." ID fNewGameRoundsList AT (56 85 AUTO AUTO) VISIBLEITEMS 8 NONUSABLE
	POPUPLIST ID fNewGameRoundsPopup fNewGameRoundsList

	LABEL "@@fNGPoints@@:" AUTOID AT (RIGHT@52 99)
	POPUPTRIGGER "" ID fNewGamePointsPopup AT (56 99 AUTO AUTO) LEFTANCHOR
	LIST "@@fNGUnlimited@@" "25" "50" "75" "100" "150" "@@fNGOther@@ ..." ID fNewGamePointsList AT (56 99 AUTO AUTO) VISIBLEITEMS 7 NONUSABLE
	POPUPLIST ID fNewGamePointsPopup fNewGamePointsList

	GRAFFITISTATEINDICATOR AT (150 150)

	BUTTON "@@fNGBegin@@" ID fNewGameBegin AT (2 BOTTOM@159 AUTO AUTO)
	BUTTON "@@fNGNames@@" ID fNewGameNames AT (PREVRIGHT+4 BOTTOM@159 AUTO AUTO)
	BUTTON "@@fNGRules@@" ID fNewGameRules AT (PREVRIGHT+4 BOTTOM@159 AUTO AUTO)
END

