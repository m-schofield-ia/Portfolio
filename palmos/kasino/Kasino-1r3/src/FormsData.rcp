MENU sysEditMenuID
BEGIN
	PULLDOWN "@@EditMenuName@@"
	BEGIN
		MENUITEM "@@EditMenuUndo@@" 10000 "U"
		MENUITEM "@@EditMenuCut@@" 10001 "X"
		MENUITEM "@@EditMenuCopy@@" 10002 "C"
		MENUITEM "@@EditMenuPaste@@" 10003 "P"
		MENUITEM "@@EditMenuSelect@@" 10004 "S"
		MENUITEM SEPARATOR
		MENUITEM "@@EditMenuKeyboard@@" 10006 "K"
		MENUITEM "@@EditMenuGraffiti@@" 10007 "G"
	END
END

MENU mMain
BEGIN
	PULLDOWN "@@GameMenuName@@"
	BEGIN
		MENUITEM "@@GameMenuNew@@" mMainNew "N"
		MENUITEM SEPARATOR
		MENUITEM "@@GameMenuNames@@" mMainNames
		MENUITEM "@@GameMenuRules@@" mMainRules
		MENUITEM SEPARATOR
		MENUITEM "@@GameMenuAbout@@" mMainAbout
	END
END

